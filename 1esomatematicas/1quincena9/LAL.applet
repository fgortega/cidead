   <param name="C_02" value="id='a.x' tipo='num&eacute;rico' regi&oacute;n='exterior' espacio='E1' nombre='a.x' valor='-6' incr='1' discreto='s&iacute;' fijo='s&iacute;' visible='s&iacute;' par&aacute;metro.fuente='Monospaced,PLAIN,12' pos_mensajes='centro'">
