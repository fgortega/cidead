   <param name="A_05" value="id='m' constante='s&iacute;' expresi&oacute;n='tan(50*pi/180)' evaluar='una-sola-vez'">
   <param name="A_06" value="id='b' constante='s&iacute;' expresi&oacute;n='-6+m' evaluar='una-sola-vez'">
