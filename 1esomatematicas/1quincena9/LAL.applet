   <param name="C_06" value="id='posterior' tipo='num&eacute;rico' interfaz='bot&oacute;n' regi&oacute;n='interior' espacio='E1' nombre='_._' expresi&oacute;n='(380,330,25,25)' fijo='s&iacute;' visible='s&iacute;' imagen='./imagenes9/sig.png' acci&oacute;n='calcular' par&aacute;metro='S=S+1' par&aacute;metro.fuente='Monospaced,PLAIN,12' dibujar-si='(a.x-4=-1)&amp;(a.y=-6)' activo-si='(a.x-4=-1)&amp;(a.y=-6)'">
   <param name="C_07" value="id='iniciar' tipo='num&eacute;rico' interfaz='bot&oacute;n' regi&oacute;n='interior' espacio='E1' nombre='_._' expresi&oacute;n='(12,330,70,25)' fijo='s&iacute;' visible='s&iacute;' imagen='vacio.gif' acci&oacute;n='inicio' par&aacute;metro.fuente='Monospaced,PLAIN,12'">
   <param name="C_08" value="id='posterior_pre' tipo='num&eacute;rico' interfaz='bot&oacute;n' regi&oacute;n='interior' espacio='E1' nombre='_._' expresi&oacute;n='(380,330,25,25)' fijo='s&iacute;' visible='s&iacute;' imagen='./imagenes9/sig.png' acci&oacute;n='calcular' par&aacute;metro='S=S+1' par&aacute;metro.fuente='Monospaced,PLAIN,12' dibujar-si='S=-1' activo-si='S=-1'">
   <param name="A_01" value="id='cox' constante='s&iacute;' expresi&oacute;n='ent(E1.rat&oacute;n_x)' evaluar='siempre'">
   <param name="A_02" value="id='coy' constante='s&iacute;' expresi&oacute;n='ent(E1.rat&oacute;n_y)' evaluar='siempre'">
   <param name="A_03" value="id='ucx' constante='s&iacute;' expresi&oacute;n='0' evaluar='siempre'">
